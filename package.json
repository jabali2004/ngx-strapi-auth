{
  "name": "ngx-strapi-auth",
  "version": "0.0.9",
  "description": "NgxStrapiAuth is a Angular library that implements all standard operations like logging in or registering a user for the headless CMS Strapi",
  "repository": {
    "type": "git",
    "url": "https://github.com/jabali2004/ngx-strapi-auth.git"
  },
  "author": {
    "name": "jabali2004",
    "url": "https://github.com/jabali2004"
  },
  "license": "Apache-2.0 License",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run develop --prefix projects/strapi-auth-showcase/strapi_backend\" \"ng serve\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build strapi-auth --prod",
    "update_version": "cd projects/strapi-auth && npm version patch",
    "npm_pack": "cd dist/strapi-auth && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "extract-translations": "ngx-translate-extract --input ./projects/strapi-auth-showcase/src --output ./projects/strapi-auth-showcase/src/assets/i18n/en.json --output ./projects/strapi-auth-showcase/src/assets/i18n/de.json --format namespaced-json  --marker . --sort",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@angular/animations": "~12.2.10",
    "@angular/cdk": "^12.2.10",
    "@angular/common": "~12.2.10",
    "@angular/compiler": "~12.2.10",
    "@angular/core": "~12.2.10",
    "@angular/forms": "~12.2.10",
    "@angular/localize": "^12.2.10",
    "@angular/material": "^12.2.10",
    "@angular/platform-browser": "~12.2.10",
    "@angular/platform-browser-dynamic": "~12.2.10",
    "@angular/router": "~12.2.10",
    "@nebular/eva-icons": "8.0.0",
    "@nebular/theme": "^8.0.0",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^5.1.0",
    "eva-icons": "^1.1.3",
    "jwt-decode": "^3.1.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "turndown": "^7.1.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.10",
    "@angular/cli": "~13.0.2",
    "@angular/compiler-cli": "~12.2.10",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^16.7.3",
    "browserlist": "^1.0.1",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.2.1",
    "husky": "7.0.4",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jshint": "^2.13.1",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.2.2",
    "ngx-translate-extract": "^1.0.0",
    "protractor": "~7.0.0",
    "standard-version": "^9.3.1",
    "strapi": "^3.6.8",
    "ts-node": "~10.4.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
