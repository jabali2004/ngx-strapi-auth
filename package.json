{
  "name": "ngx-strapi-auth",
  "version": "0.2.10",
  "description": "NgxStrapiAuth is a Angular library that implements all standard operations like logging in or registering a user for the headless CMS Strapi",
  "repository": {
    "type": "git",
    "url": "https://github.com/jabali2004/ngx-strapi-auth.git"
  },
  "author": {
    "name": "jabali2004",
    "url": "https://github.com/jabali2004"
  },
  "license": "Apache-2.0 License",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run develop --prefix ./backend\" \"ng serve\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build strapi-auth",
    "update_version": "cd projects/strapi-auth && npm version patch",
    "npm_pack": "cd dist/strapi-auth && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "extract-translations": "ngx-translate-extract --input ./projects/strapi-auth-example/src --output ./projects/strapi-auth-example/src/assets/i18n/en.json --output ./projects/strapi-auth-example/src/assets/i18n/de.json --format namespaced-json  --marker . --sort",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "dependencies": {
    "@angular/animations": "^14.1.1",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "^14.1.1",
    "@angular/compiler": "^14.1.1",
    "@angular/core": "^14.1.1",
    "@angular/forms": "^14.1.1",
    "@angular/localize": "^14.1.1",
    "@angular/material": "^15.0.1",
    "@angular/platform-browser": "^14.1.1",
    "@angular/platform-browser-dynamic": "^14.1.1",
    "@angular/router": "^14.1.1",
    "@compodoc/compodoc": "^1.1.19",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.2.0",
    "jwt-decode": "^3.1.2",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular-eslint/builder": "14.1.1",
    "@angular-eslint/eslint-plugin": "14.1.1",
    "@angular-eslint/eslint-plugin-template": "14.1.1",
    "@angular-eslint/schematics": "14.1.1",
    "@angular-eslint/template-parser": "14.1.1",
    "@angular/cli": "^14.1.1",
    "@angular/compiler-cli": "^14.1.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "browserlist": "^1.0.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8.0.1",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jshint": "^2.13.5",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^14.1.0",
    "ngx-translate-extract": "^1.0.0",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "standard-version": "^9.5.0",
    "ts-node": "~10.9.1",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
